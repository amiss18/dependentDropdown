{% extends 'base.html.twig' %}

{% block title %}Commune{% endblock %}

{% block body %}
    <h1>Commune</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ commune.id }}</td>
            </tr>
            <tr>
                <th>Lon</th>
                <td>{{ commune.lon }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ commune.lat }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ commune.nom }}</td>
            </tr>
            <tr>
                <th>CodeDepartement</th>
                <td>{{ commune.codeDepartement }}</td>
            </tr>
            <tr>
                <th>CodeRegion</th>
                <td>{{ commune.codeRegion }}</td>
            </tr>
            <tr>
                <th>Population</th>
                <td>{{ commune.population }}</td>
            </tr>
            <tr>
                <th>CodesPostaux</th>
                <td>{{ commune.codesPostaux ? commune.codesPostaux|join(', ') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('commune_index') }}">back to list</a>

    <a href="{{ path('commune_edit', {'id': commune.id}) }}">edit</a>

    {{ include('commune/_delete_form.html.twig') }}
{% endblock %}
